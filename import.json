{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "s3import",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::s3-to-rds-postgres",
        "arn:aws:s3:::s3-to-rds-postgres/*"
      ]
    }
  ]
}